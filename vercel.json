{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*\\.(html|css|js|ico|png|jpg|svg|woff2?))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}